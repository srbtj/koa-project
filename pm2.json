{
  "apps": [
    {
      "name": "App",
      "script": "cross-env NODE_ENV=development app.js",
      "watch": true,
      "log_data_format": "YYYY-MM-DD HH:mm:ss z",
      "instances": "max",
      "exec_mode": "cluster",
      "error_file": "./logs/error_out.log",
      "env": {
        "NODE_ENV": "development"
      },
      "env_production": {
        "NODE_ENV": "production"
      }
    }
  ]
}
